{
  "name": "progue",
  "version": "0.0.0",
  "private": true,
  "author": {
    "name": "Ochuko Awhe",
    "email": ""
  },
  "workspaces": [
    "apps/*",
    "packages/*",
    "web3/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --paralell",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "test:watch": "turbo run test:watch",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "eslint-config-custom": "*",
    "lint-staged": "^12.1.3",
    "prettier": "latest",
    "turbo": "^1.10.12"
  },
  "lint-staged": {
    "apps/**/*.{js,ts,jsx,tsx}": [
      "eslint --fix"
    ],
    "packages/ui/**/*.{js,ts,jsx,tsx}": [
      "eslint --fix"
    ],
    "*.json": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "yarn@1.22.19",
  "dependencies": {
    "react-hook-form": "^7.45.2"
  }
}
